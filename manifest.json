{
  "manifest_version": 2,

  "name": "SWC Journeys",
  "description": "Save bookmark",
  "version": "1.0.0",
  "background": {
    "service_worker": "background.js"
  },
  "icons": { "128": "Group.png" },
  "browser_action": {
    "default_icon": "Group.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "cookies", "http://*/*","https://*/*"]
}
